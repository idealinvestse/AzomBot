# Example environment for AZOM AI Agent
# Copy to .env and adjust values as needed.

# Core API settings
APP_NAME=AZOM AI Agent
APP_VERSION=1.0.0
DEBUG=false
HOST=0.0.0.0
PORT=8008

# External integrations
LLM_BACKEND=openwebui  # or 'groq' or 'openai'
GROQ_API_KEY=
GROQ_API_URL=https://api.groq.com/openai/v1/chat/completions
GROQ_MODEL=qwen/qwen3-32b
OPENWEBUI_API_TOKEN=
OPENWEBUI_URL=http://localhost:3000
OPENAI_API_KEY=
OPENAI_BASE_URL=https://api.openai.com/v1
TARGET_MODEL=azom-ai-general

# CORS (Core API)
# Comma-separated list of allowed origins, e.g. https://your-wordpress-site.com
CORS_ORIGINS=https://localhost:3000,https://example.com

# Pipeline server settings
PROJECT_NAME=AZOM Pipeline Server
PIPELINE_PORT=8001
DEFAULT_USER_EXPERIENCE=nyb√∂rjare
CACHE_TTL_SECONDS=3600

# CORS (Pipeline Server)
# Same format as above; used by app/pipelineserver/pipeline_app/main.py
PIPELINE_CORS_ORIGINS=https://localhost:3000,https://example.com
